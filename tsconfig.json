{
  "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "inlineSourceMap": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strictNullChecks": false
  },
  "typeRoots": ["@types", "./node_modules/@types"],
  "exclude": [
      "node_modules", "storage"
  ]
}
